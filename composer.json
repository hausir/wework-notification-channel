{
    "name": "hausir/wework-notification-channel",
    "description": "Wechat Work Notification Channel for laravel.",
    "keywords": ["laravel", "notifications", "wework"],
    "license": "MIT",
    "authors": [
        {
            "name": "hausir",
            "email": "hausir@icloud.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/notifications": "~5.8.0|~5.9.0",
        "overtrue/wechat": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\WeWork\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\WeWork\\WeWorkChannelServiceProvider"
            ]
        }
    }
}
